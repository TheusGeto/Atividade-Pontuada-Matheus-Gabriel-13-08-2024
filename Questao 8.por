programa {
  funcao inicio() {
    cadeia cor
    escreva("digite a cor do tipo de cd( tudo minusculo): ")
    leia(cor)

    se(cor == "verde"){ escreva("o disco em questão custa R$ 10,00")}

    se(cor == "azul"){ escreva("o disco em questão custa R$ 20,00")}

    se(cor == "amarelo"){ escreva("o disco em questão custa R$ 30,00")}

    se(cor == "vermelho"){ escreva("o disco em questão custa R$ 40,00")}

    
  }
}
